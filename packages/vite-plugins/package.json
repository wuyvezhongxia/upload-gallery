{
  "name": "@yuanjing/tinypng-plugin",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup src/index.ts --format esm --dts",
    "dev": "cross-env NODE_ENV=development tsup src/index.ts --format esm --dts --watch"
  },
  "keywords": [
    "vite",
    "plugin",
    "tinypng",
    "image",
    "compression"
  ],
  "description": "Vite plugin for TinyPNG image compression",
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.6.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "p-limit": "^6.2.0"
  }
}